<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:loc="clr-namespace:LogService.Client.Localisation;assembly=LogService.Client.Localisation"
			 mc:Ignorable="d"
			 Title="Menu"
			 x:Class="LogService.Maui.View.MenuView">
	
	<ContentPage.Content>
		<StackLayout>
			<Button Text="LogMessages" Command="{Binding NavigateToLogMessages}"
					HorizontalOptions="FillAndExpand" Margin="1" BackgroundColor="#00375a" TextColor="#ffffff" />
		</StackLayout>
	</ContentPage.Content>
</ContentPage>